<Window x:Class="UniversalQueryFrameWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <TextBlock Text="{Binding Path=Query_Name}" Height="20" Width="100"></TextBlock>
        <Button Command="{Binding Query_Command_Query}" Height="20" Width="100"></Button>
    </DockPanel>
</Window>
